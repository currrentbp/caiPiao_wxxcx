.container {
  height: 100%;
  width:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 5rpx 0;
  box-sizing: border-box;
}

.next {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 500rpx 0;
  box-sizing: border-box;
}

.red {
  display: inline-block;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  color: #FFFFFF;
  text-align: center;
  background: red;
}

.blue {
  display: inline-block;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  color: #FFFFFF;
  text-align: center;
  background: #0066FF;
}